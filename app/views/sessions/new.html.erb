
<%= form_for(:session, url: sessions_path) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Sign In", class: 'btn btn-large btn-primary' %>
<% end %>

<p> New User? <%= link_to "Sign Up Here", new_user_path %></p>